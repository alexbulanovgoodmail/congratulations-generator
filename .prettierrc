{
	"semi": false,
	"singleQuote": false,
	"tabWidth": 2,
	"useTabs": true,
	"trailingComma": "none",
	"printWidth": 120,
	"bracketSpacing": true,
	"arrowParens": "avoid",
	"endOfLine": "auto",
	"jsxBracketSameLine": false,
	"jsxSingleQuote": false,
	"vueIndentScriptAndStyle": false,
	"plugins": ["prettier-plugin-tailwindcss"]
}
